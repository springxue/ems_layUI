<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.casic.mapper.DevUserMapper">
    <select id="findDevUser" resultType="com.casic.model.DevUser">
      select * from dev_user where devName=#{devName} and devPassword=#{devPassword}
    </select>
</mapper>